[ENABLE]
alloc(Hook,64)


Hook:
call 004DE8F0 // Original Call
mov ecx,eax
mov eax,[esp+0C]
mov edi,[020B7C9C] // CUserLocal: 8B 3D ? ? ? ? 8B 40
mov edi,[edi+E8DC] // Character X Location Offset: 89 8E ? ? ? ? 8B 50 ? 8B 06 89 96 ? ? ? ? 8B 50
add edi,#0 // X
mov [eax],edi
pop edi
mov ecx,[020B7C9C]
mov ecx,[ecx+E8DC+04]
add ecx,#0 // Y
mov [eax+04],ecx
pop esi
ret 0004


00B279A4:
jmp Hook
